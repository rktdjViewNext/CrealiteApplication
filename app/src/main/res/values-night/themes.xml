<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.CrealiteApp" parent="Theme.Material3.Dark.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/orange</item>
        <item name="colorPrimaryVariant">@color/orange</item>
        <item name="colorOnPrimary">@color/grey_li</item>

        <item name="colorSecondary">@color/orange</item>
        <item name="colorOnSecondary">@color/orange</item>

        <item name="android:statusBarColor">@color/orange</item>
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Base.Theme.CrealiteApp2" parent="Theme.AppCompat.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/orange</item>
        <item name="colorPrimaryVariant">@color/grey</item>
        <item name="colorOnPrimary">@color/grey_li</item>

        <item name="colorSecondary">@color/orange</item>
        <item name="colorOnSecondary">@color/black</item>

        <item name="android:statusBarColor">@color/orange</item>
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.CrealiteApp" parent="Base.Theme.CrealiteApp" />

    <style name="roundedImageViewRounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


</resources>